<div class="panel panel-primary">
  <div class="panel-heading">
    <h3 class="panel-title"><%= t('nuevo_elemento') %></h3>
  </div>
  <%= form_for(@camino) do |f| %>
    <div class="panel-body">
      <div class="form-group">
        <%= f.label :nombre %>
        <%= f.text_field :nombre, class:'form-control'%>
      </div>
      <div class="form-group">
        <%= f.label :inicio %>
        <%= f.text_field :inicio, class:'form-control' %>
      </div>
      <div class="form-group">
        <%= f.label :fin %>
        <%= f.text_field :fin, class:'form-control' %>
      </div>
    </div>
    <div class="panel-footer">
      <%= f.submit class: 'btn btn-primary' %>
    </div>
  <% end %>
</div>
